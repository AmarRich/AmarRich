	void dfs(vector<vector<int>> &ans,vector<int> &curr,int i,int n){
  //   when n is 0 than just insert inside it in ans
	    if(n==0){ ans.push_back(curr); return; }
	    if(i==0) return;
	    if(i<=n) {
	        curr.push_back(i); 
	        dfs(ans,curr,i,n-i);
	        curr.pop_back();
	    }
	    dfs(ans,curr,i-1,n);
	}
	
    vector<vector<int>> UniquePartitions(int n) {
        // Code here
        vector<vector<int>> ans;
        vector<int> curr;
        dfs(ans,curr,n,n);
        return ans;
        }
        


